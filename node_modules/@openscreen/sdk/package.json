{
  "name": "@openscreen/sdk",
  "version": "0.7.1",
  "description": "Openscreen Software Development Kit (SDK)",
  "homepage": "https://github.com/openscreen-tv/openscreen-sdk",
  "keywords": [
    "qr",
    "qrcode",
    "qr-code",
    "QR Code",
    "QR Code Generator",
    "barcode",
    "node.js"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/openscreen-tv/openscreen-sdk.git"
  },
  "license": "Apache-2.0",
  "author": {
    "name": "Openscreen",
    "url": "https://openscreen.com"
  },
  "exports": {
    "import": "./dist/index.esm.mjs",
    "require": "./dist/index.js",
    "default": "./dist/index.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.modern.js",
  "source": "./src/index.ts",
  "types": "./typings/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "scripts": {
    "generate-sdk": "./scripts/generate-api-handlers-list > ./scripts/api-handlers-list.js; ./scripts/generate-sdk --output src/sdk.ts --pkg sdk --pkg accounts",
    "generate-oai-spec": "./scripts/generate-api-handlers-list > ./scripts/api-handlers-list.js; ./scripts/generate-oai-spec.js --output src/oai-spec.json --pkg sdk",
    "build": "microbundle --format=modern,esm,cjs",
    "build:watch": "tsc --watch",
    "lint": "eslint src",
    "test": "jest",
    "test2": "jest --help",
    "test:watch": "jest --watch"
  },
  "dependencies": {
    "amazon-cognito-identity-js": "5.0.3",
    "axios": "0.21.2",
    "moment": "2.29.1"
  },
  "devDependencies": {
    "@core/schema": "~0.0.1",
    "@types/jest": "26.0.23",
    "@types/node": "15.3.1",
    "@typescript-eslint/eslint-plugin": "4.29.3",
    "@typescript-eslint/parser": "4.29.3",
    "dot-prop": "6.0.1",
    "dotenv": "10.0.0",
    "esbuild-jest": "0.5.0",
    "eslint": "7.26.0",
    "eslint-config-airbnb-typescript": "14.0.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.23.2",
    "eslint-plugin-jest": "25.0.5",
    "graceful-fs": "4.2.8",
    "jest": "27.3.1",
    "microbundle": "0.14.1",
    "typescript": "4.3.2",
    "write-file-atomic": "3.0.3",
    "esm": "3.2.25",
    "lodash": "4.17.21",
    "yargs": "13.3.2"
  }
}
